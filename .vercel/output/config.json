{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/callback\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth\\/signout\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/stories\\/create\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/stories\\/([^/]+?)\\/delete\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/stories\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/signin\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/story\\/([^/]+?)\\/chapter\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/story\\/([^/]+?)\\/lorebook\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/story\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}